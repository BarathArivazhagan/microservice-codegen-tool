{
	
	"swagger": "2.0",
	"info": {
		"description": "Product API reference for developers",
		"version": "1.0"		
	},
	"entities": {
		"Product": {

			"properties": {
				"productId": {
					"type": "Long",
					"primaryKey": true,
					"isAutoGenerated": true

				},
				"productName": {
					"type": "String"
					
				},
				"brand": {
					"type": "Brand",
					"relation": {
						"ManyToOne": true
					},
					"joinColumn":"brandId"
				}
				
			}
		},
		"Brand": {

			"properties": {
				"brandId": {
					"type": "Long",
					"primaryKey": true,
					"isAutoGenerated": true

				},
				"brandName": {
					"type": "String"
				},
				"product": {
					"type": "Product",
					"isSet": true,
					"relation": {
						"OneToMany": true
					},
					"mappedBy":"brand"
				}




			}
		}




	},
	"properties": {

		"spring.datasource.url": "test",
		"spring.datasource.username": "test",
		"spring.datasource.password": "test"
	}
}